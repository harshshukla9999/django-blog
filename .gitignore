# Ignore files generated by Django
*.pyc
*.pyo
__pycache__

# Ignore the settings file (specifically, use your own settings filename if different)
/blog/settings.py

# Ignore the database file (if using SQLite)
db.sqlite3

# Ignore the media and static files (if using file storage)
/media/*
/static/*

# Ignore the local development configuration file (if any)
/local_settings.py

# Ignore IDE or editor specific files
.vscode/
.idea/

# Ignore any virtual environment folder (assuming you are using venv or virtualenv)
/blogenv/
.env

# Ignore the database configuration file (if using PostgreSQL, MySQL, etc.)
/db_config.json

# Ignore any secret or sensitive information
/secrets.json
/secret_key.txt
